<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
	configVersion="3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://www.phpdoc.org"
	xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd"
>
	<title>PHosPhore Documentation</title>
	<paths>
		<output>phpDocumentor/output</output>
		<cache>phpDocumentor/cache</cache>
	</paths>
	<version number="3.0.0">
		<api>
			<source dsn="../">
				<path>src</path>
			</source>
			<default-package-name>PHosPHore</default-package-name>
			<include-source>true</include-source>
		</api>
	</version>
	<template name="phpDocumentor/template-default/template.xml" location="phpDocumentor/template-default" >
	</template>
</phpdocumentor>
